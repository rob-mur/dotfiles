{
    "initial_working_directory": "~",
    "editor_theme": "Tomorrow Night",
    "pdf_previewer": "none",
    "posix_terminal_shell": "custom",
    "custom_shell_command": "/run/current-system/sw/bin/fish",
    "jobs_tab_visibility": "shown",
    "global_theme": "alternate",
    "rmd_viewer_type": "pane",
    "panes": {
        "quadrants": [
            "TabSet1",
            "TabSet2",
            "Source",
            "Console"
        ],
        "tabSet1": [
            "Environment",
            "History",
            "Connections",
            "Build",
            "VCS",
            "Tutorial",
            "Presentation"
        ],
        "tabSet2": [
            "Files",
            "Plots",
            "Packages",
            "Help",
            "Viewer",
            "Presentations"
        ],
        "hiddenTabSet": [],
        "console_left_on_top": false,
        "console_right_on_top": false,
        "additional_source_columns": 0
    },
    "soft_wrap_r_files": true,
    "highlight_selected_line": true,
    "relative_line_numbers": true,
    "auto_append_newline": true,
    "warn_if_no_such_variable_in_scope": true,
    "warn_variable_defined_but_not_used": true
}